{
    "Metrics": {
        "Port": "9090"
    },
    "Log": {
        "Human": false,
        "Debug": true
    },
    "Target": "https://testnet.tableland.network/rpc",
    "Chains": [
        {
            "Name": "optimism-kovan",
            "Probe": {
                "CheckInterval": "120s",
                "ReceiptTimeout": "25s",
                "SIWE": "${HEALTHBOT_OPTIMISM_KOVAN_SIWE}",
                "Tablename": "${HEALTHBOT_OPTIMISM_KOVAN_TABLE}"
            }
        },
        {
            "Name": "arbitrum-goerli",
            "Probe": {
                "CheckInterval": "120s",
                "ReceiptTimeout": "25s",
                "SIWE": "${HEALTHBOT_ARBITRUM_GOERLI_SIWE}",
                "Tablename": "${HEALTHBOT_ARBITRUM_GOERLI_TABLE}"
            }
        },
        {
            "Name": "ethereum-goerli",
            "Probe": {
                "CheckInterval": "1h",
                "ReceiptTimeout": "90s",
                "SIWE": "${HEALTHBOT_ETHEREUM_GOERLI_SIWE}",
                "Tablename": "${HEALTHBOT_ETHEREUM_GOERLI_TABLE}"
            }
        },
        {
            "Name": "polygon-mumbai",
            "Probe": {
                "CheckInterval": "240s",
                "ReceiptTimeout": "40s",
                "SIWE": "${HEALTHBOT_POLYGON_MUMBAI_SIWE}",
                "Tablename": "${HEALTHBOT_POLYGON_MUMBAI_TABLE}"
            }
        }
    ]
}