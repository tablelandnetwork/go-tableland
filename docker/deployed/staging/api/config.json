{
  "HTTP": {
    "Port": "8080",
    "RateLimInterval": "1s",
    "MaxRequestPerInterval": 10,
    "TLSCert": "${VALIDATOR_TLS_CERT}",
    "TLSKey": "${VALIDATOR_TLS_KEY}"
  },
  "Gateway": {
    "ExternalURIPrefix": "https://staging.tableland.network",
    "MetadataRendererURI": ""
  },
  "DB": {
    "Port": "5432"
  },
  "TableConstraints": {
    "MaxRowCount": 500000
  },
  "QueryConstraints": {
    "MaxWriteQuerySize": 35000,
    "MaxReadQuerySize": 35000
  },
  "Metrics": {
    "Port": "9090"
  },
  "Log": {
    "Human": false,
    "Debug": true
  },
  "Analytics": {
    "FetchExtraBlockInfo": false
  },
  "Backup" : {
		"Enable": true,            
		"Dir": "backups",               
		"Frequency": 2,         
		"EnableVacuum": true,      
		"EnableCompression": true, 
		"EnablePruning": true,     
		"KeepFiles": 5        
	},
  "Chains": [
    {
      "Name": "Optimism Kovan",
      "AllowTransactionRelay": true,
      "ChainID": 69,
      "Registry": {
        "EthEndpoint": "wss://opt-kovan.g.alchemy.com/v2/${VALIDATOR_ALCHEMY_OPTIMISM_KOVAN_API_KEY}",
        "ContractAddress": "0x2b51D72F210A96B6aB8EfB02dF8802363dcC83Dd"
      },
      "Signer": {
        "PrivateKey": "${VALIDATOR_OPTIMISM_KOVAN_SIGNER_PRIVATE_KEY}"
      },
      "EventFeed": {
        "ChainAPIBackoff": "15s",
        "NewBlockTimeout": "30s",
        "MinBlockDepth": 0,
        "PersistEvents": false
      },
      "EventProcessor": {
        "BlockFailedExecutionBackoff": "10s",
        "DedupExecutedTxns": true
      },
      "NonceTracker": {
        "CheckInterval": "10s",
        "StuckInterval": "10m",
        "MinBlockDepth": 0
      }
    },
    {
      "Name": "Optimism Goerli",
      "AllowTransactionRelay": true,
      "ChainID": 420,
      "Registry": {
        "EthEndpoint": "wss://opt-goerli.g.alchemy.com/v2/${VALIDATOR_ALCHEMY_OPTIMISM_GOERLI_API_KEY}",
        "ContractAddress": "0xfe79824f6E5894a3DD86908e637B7B4AF57eEE28"
      },
      "Signer": {
        "PrivateKey": "${VALIDATOR_OPTIMISM_GOERLI_SIGNER_PRIVATE_KEY}"
      },
      "EventFeed": {
        "ChainAPIBackoff": "15s",
        "NewBlockTimeout": "30s",
        "MinBlockDepth": 0,
        "PersistEvents": false
      },
      "EventProcessor": {
        "BlockFailedExecutionBackoff": "10s",
        "DedupExecutedTxns": true
      },
      "NonceTracker": {
        "CheckInterval": "10s",
        "StuckInterval": "10m",
        "MinBlockDepth": 0
      }
    }
  ]
}